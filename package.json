{
  "name": "rin",
  "version": "1.0.0",
  "workspaces": [
    "server",
    "client"
  ],
  "private": true,
  "scripts": {
    "dev": "turbo dev",
    "dev:client": "bun --filter './client' dev",
    "dev:server": "bun --filter './server' dev",
    "check": "turbo check",
    "cf-deploy": "bun server/migrator.ts",
    "b": "turbo build",
    "t": "turbo t",
    "g": "turbo run g",
    "m": "bun scripts/dev-migrator.ts",
    "clean": "rm -rf dist node_modules client/node_modules server/node_modules",
    "format:check": "turbo format:check",
    "format:write": "turbo format:write"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.19",
    "postcss": "^8.4.38",
    "puppeteer": "^22.10.0",
    "tailwindcss": "^3.4.3",
    "turbo": "^1.13.3",
    "vitest": "1.3.0"
  },
  "dependencies": {
    "fast-xml-parser": "^4.4.0",
    "html-to-md": "^0.8.5"
  }
}
